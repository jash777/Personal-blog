{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/app.py"
      }
    ],
    "env": {
      "VIRTUAL_ENV": "/vercel/path0/venv",
      "PATH": "/vercel/path0/venv/bin:$PATH"
    }
  }
  